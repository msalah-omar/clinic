<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">
    <changeSet id="3" author="mohamed salah">
        <createTable tableName="clinic">
            <column autoIncrement="true" name="id" type="INT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_clinic"/>
            </column>
            <column name="version" type="INT"/>
            <column name="created_by" type="VARCHAR(255)"/>
            <column name="modified_by" type="VARCHAR(255)"/>
            <column name="created_date" type="DATETIME"/>
            <column name="modified_date" type="DATETIME"/>
            <column name="name" type="varchar(255)"/>
            <column name="working_days" type="varchar(255)"/>
            <column name="phone_number" type="varchar(20)"/>
            <column name="email" type="varchar(255)"/>
            <column name="address" type="varchar(255)"/>
            <column name="social_networks_urls" type="varchar(255)"/>

        </createTable>
    </changeSet>
</databaseChangeLog>
